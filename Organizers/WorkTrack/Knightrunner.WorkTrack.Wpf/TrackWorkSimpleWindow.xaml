<Window x:Class="Knightrunner.WorkTrack.Wpf.TrackWorkSimpleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="What are you doing?" Height="163" Width="431" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded">
    <Grid>
        <Label Content="Text" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <Button Content="OK" Height="23" Margin="241,0,0,12" Name="buttonOk" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="75" IsDefault="True" Click="buttonOk_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="0,0,12,12" Name="buttonCancel" VerticalAlignment="Bottom" Width="75" IsCancel="True" />
        <ComboBox Height="23" HorizontalAlignment="Stretch" Margin="12,36,12,0" Name="comboBoxText" VerticalAlignment="Top" IsEditable="True" />
    </Grid>
</Window>
